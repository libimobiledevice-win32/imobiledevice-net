{
  "version": "1.2.1-*",

  "authors": [
    "Quamotion"
  ],

  "title": ".NET client for iOS devices (libimobiledevice-net)",
  "description": ".NET bindings for libimobiledevice, allows you to interact with devices running iOS, such as iPhones or iPads, using C# or VB.NET code",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/libimobiledevice-win32/imobiledevice-net/blob/master/LICENSE.txt",
    "owners": [
      "Quamotion"
    ],
    "repository": {
      "type": "git",
      "url": "http://github.com/libimobiledevice-win32/imobiledevice-net"
    },
    "tags": [ "libimobiledevice", "ios", "iphone", "ipad", "imobiledevice", "netcore" ]
  },

  "scripts": {
  },

  "dependencies": {
    "runtime.linux-x64.iMobileDevice-net": "1.2.1-beta009",
    "runtime.osx-x64.iMobileDevice-net": "1.2.1-beta003",
    "runtime.win-x86.iMobileDevice-net": "1.2.1-r144",
    "runtime.win-x64.iMobileDevice-net": "1.2.1-r144"
  },

  "buildOptions": {
    "strongName": "true",
    "keyFile": "iMobileDevice.snk"
  },

  "frameworks": {
    "netstandard1.5": {
      "dependencies": {
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Runtime.InteropServices.RuntimeInformation": "4.0.0",
        "System.Linq": "4.1.0",
        "System.Diagnostics.Debug": "4.0.11",
        "System.IO.FileSystem": "4.0.1",
        "System.Diagnostics.StackTrace": "4.0.1",
        "System.Threading":  "4.0.11",
        "Microsoft.Win32.Primitives": "4.0.1"
      }
    },
    "net45": {
      "frameworkAssemblies": {
      }
    }
  }
}